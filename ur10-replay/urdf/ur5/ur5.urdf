<?xml version="1.0"?>
<robot name="ur5">

  <!-- Base frames -->
  <link name="base_link"/>

  <link name="base">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="meshes/visual/base.dae"/>
        <!-- If meshes are in mm, add: scale="0.001 0.001 0.001" -->
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="meshes/collision/base.stl"/>
      </geometry>
    </collision>
  </link>

  <joint name="base_link-base_fixed_joint" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="base_link"/>
    <child link="base"/>
  </joint>

  <!-- J1 shoulder_pan -->
  <link name="shoulder_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="meshes/visual/shoulder.dae"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="meshes/collision/shoulder.stl"/>
      </geometry>
    </collision>
  </link>

  <joint name="shoulder_pan_joint" type="revolute">
    <origin xyz="0 0 0.089159" rpy="0 0 0"/>
    <parent link="base"/>
    <child link="shoulder_link"/>
    <axis xyz="0 0 1"/>
    <limit lower="-6.28318530718" upper="6.28318530718" velocity="3.15" effort="150.0"/>
  </joint>

  <!-- J2 shoulder_lift -->
  <link name="upper_arm_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="meshes/visual/upperarm.dae"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="meshes/collision/upperarm.stl"/>
      </geometry>
    </collision>
  </link>

  <joint name="shoulder_lift_joint" type="revolute">
    <origin xyz="0 0.13585 0" rpy="0 1.57079632679 0"/>
    <parent link="shoulder_link"/>
    <child link="upper_arm_link"/>
    <axis xyz="0 1 0"/>
    <limit lower="-6.28318530718" upper="6.28318530718" velocity="3.15" effort="150.0"/>
  </joint>

  <!-- J3 elbow -->
  <link name="forearm_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="meshes/visual/forearm.dae"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="meshes/collision/forearm.stl"/>
      </geometry>
    </collision>
  </link>

  <joint name="elbow_joint" type="revolute">
    <origin xyz="0 -0.1197 0.425" rpy="0 0 0"/>
    <parent link="upper_arm_link"/>
    <child link="forearm_link"/>
    <axis xyz="0 1 0"/>
    <limit lower="-3.14159265359" upper="3.14159265359" velocity="3.15" effort="150.0"/>
  </joint>

  <!-- J4 wrist_1 -->
  <link name="wrist_1_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="meshes/visual/wrist1.dae"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="meshes/collision/wrist1.stl"/>
      </geometry>
    </collision>
  </link>

  <joint name="wrist_1_joint" type="revolute">
    <origin xyz="0 0 0.39225" rpy="0 1.57079632679 0"/>
    <parent link="forearm_link"/>
    <child link="wrist_1_link"/>
    <axis xyz="0 1 0"/>
    <limit lower="-6.28318530718" upper="6.28318530718" velocity="3.2" effort="28.0"/>
  </joint>

  <!-- J5 wrist_2 -->
  <link name="wrist_2_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="meshes/visual/wrist2.dae"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="meshes/collision/wrist2.stl"/>
      </geometry>
    </collision>
  </link>

  <joint name="wrist_2_joint" type="revolute">
    <origin xyz="0 0.093 0" rpy="0 0 0"/>
    <parent link="wrist_1_link"/>
    <child link="wrist_2_link"/>
    <axis xyz="0 0 1"/>
    <limit lower="-6.28318530718" upper="6.28318530718" velocity="3.2" effort="28.0"/>
  </joint>

  <!-- J6 wrist_3 -->
  <link name="wrist_3_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="meshes/visual/wrist3.dae"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="meshes/collision/wrist3.stl"/>
      </geometry>
    </collision>
  </link>

  <joint name="wrist_3_joint" type="revolute">
    <origin xyz="0 0 0.09465" rpy="0 0 0"/>
    <parent link="wrist_2_link"/>
    <child link="wrist_3_link"/>
    <axis xyz="0 1 0"/>
    <limit lower="-6.28318530718" upper="6.28318530718" velocity="3.2" effort="28.0"/>
  </joint>

  <!-- Tool flange -->
  <link name="tool0"/>
  <joint name="wrist_3-tool0" type="fixed">
    <origin xyz="0 0.0823 0" rpy="-1.57079632679 0 0"/>
    <parent link="wrist_3_link"/>
    <child link="tool0"/>
  </joint>

</robot>
