<?xml version="1.0" ?>
<!-- UR5e Robot - With Official Mesh Files -->
<robot name="ur5e">
  <!-- Base Link -->
  <link name="base_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="meshes/ur5e/visual/base.dae"/>
      </geometry>
    </visual>
  </link>

  <!-- Shoulder Pan Joint -->
  <joint name="shoulder_pan_joint" type="revolute">
    <parent link="base_link"/>
    <child link="shoulder_link"/>
    <origin xyz="0 0 0.1625" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-6.2832" upper="6.2832" effort="330.0" velocity="2.0944"/>
  </joint>

  <link name="shoulder_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="meshes/ur5e/visual/shoulder.dae"/>
      </geometry>
    </visual>
  </link>

  <!-- Shoulder Lift Joint -->
  <joint name="shoulder_lift_joint" type="revolute">
    <parent link="shoulder_link"/>
    <child link="upper_arm_link"/>
    <origin xyz="0 0.13850 0" rpy="0 1.570796325 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-6.2832" upper="6.2832" effort="330.0" velocity="2.0944"/>
  </joint>

  <link name="upper_arm_link">
    <visual>
      <origin xyz="0 0 0.2125" rpy="0 1.570796325 0"/>
      <geometry>
        <mesh filename="meshes/ur5e/visual/upperarm.dae"/>
      </geometry>
    </visual>
  </link>

  <!-- Elbow Joint -->
  <joint name="elbow_joint" type="revolute">
    <parent link="upper_arm_link"/>
    <child link="forearm_link"/>
    <origin xyz="0 -0.1197 0.425" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-3.1416" upper="3.1416" effort="150.0" velocity="3.1416"/>
  </joint>

  <link name="forearm_link">
    <visual>
      <origin xyz="0 0 0.19615" rpy="0 1.570796325 0"/>
      <geometry>
        <mesh filename="meshes/ur5e/visual/forearm.dae"/>
      </geometry>
    </visual>
  </link>

  <!-- Wrist 1 Joint -->
  <joint name="wrist_1_joint" type="revolute">
    <parent link="forearm_link"/>
    <child link="wrist_1_link"/>
    <origin xyz="0 0 0.3923" rpy="0 1.570796325 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-6.2832" upper="6.2832" effort="54.0" velocity="3.1416"/>
  </joint>

  <link name="wrist_1_link">
    <visual>
      <origin xyz="0 0.09465 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="meshes/ur5e/visual/wrist1.dae"/>
      </geometry>
    </visual>
  </link>

  <!-- Wrist 2 Joint -->
  <joint name="wrist_2_joint" type="revolute">
    <parent link="wrist_1_link"/>
    <child link="wrist_2_link"/>
    <origin xyz="0 0.09465 0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-6.2832" upper="6.2832" effort="54.0" velocity="3.1416"/>
  </joint>

  <link name="wrist_2_link">
    <visual>
      <origin xyz="0 0 0.09465" rpy="0 0 0"/>
      <geometry>
        <mesh filename="meshes/ur5e/visual/wrist2.dae"/>
      </geometry>
    </visual>
  </link>

  <!-- Wrist 3 Joint -->
  <joint name="wrist_3_joint" type="revolute">
    <parent link="wrist_2_link"/>
    <child link="wrist_3_link"/>
    <origin xyz="0 0 0.09465" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-6.2832" upper="6.2832" effort="54.0" velocity="3.1416"/>
  </joint>

  <link name="wrist_3_link">
    <visual>
      <origin xyz="0 0.0823 0" rpy="0 1.570796325 0"/>
      <geometry>
        <mesh filename="meshes/ur5e/visual/wrist3.dae"/>
      </geometry>
    </visual>
  </link>

  <!-- Tool0 (end effector reference frame) -->
  <joint name="tool0_joint" type="fixed">
    <parent link="wrist_3_link"/>
    <child link="tool0"/>
    <origin xyz="0 0.0823 0" rpy="0 0 0"/>
  </joint>

  <link name="tool0"/>

</robot>
